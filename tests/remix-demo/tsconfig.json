{
  "include": ["src", "vite.config.ts", "app/combobox.tsx"],
  "compilerOptions": {
    "lib": ["DOM", "DOM.Iterable", "ES2022"],
    "isolatedModules": true,
    "esModuleInterop": true,
    "jsx": "react-jsx",
    "moduleResolution": "bundler",
    "module": "ESNext",
    "resolveJsonModule": true,
    "target": "ES2022",
    "strict": true,
    "types": ["vite/client", "vitest/globals"],
    "noEmit": true
  }
}
